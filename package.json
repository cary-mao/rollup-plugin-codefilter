{
  "name": "rollup-plugin-codefilter",
  "version": "v1.0.0-beta",
  "description": "A plugin for rollup/vite to filter code by dotenv variables",
  "main": "dist/index.cjs.js",
  "module": "index.js",
  "repository": {
    "url": "https://github.com/cary-mao/rollup-plugin-codefilter"
  },
  "scripts": {
    "build": "npx rollup -c rollup.config.js",
    "test": "npx ava test/test.js",
    "publish": "npx np"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@types/node": "^15.6.0",
    "ava": "^3.15.0",
    "cz-conventional-changelog": "^3.3.0",
    "esm": "^3.2.25",
    "np": "^7.5.0",
    "rollup": "^2.48.0",
    "vite": "^2.3.3"
  },
  "license": "MIT",
  "keywords": [
    "rollup-plugin",
    "vite-plugin"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0",
    "chalk": "^4.1.1",
    "dotenv": "^10.0.0",
    "parse-json": "^5.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
